{
	"name": "swap-ternary",
	"displayName": "SwapTernary",
	"description": "Swap the outcome of a ternary.",
	"publisher": "256hz",
	"version": "0.1.7",
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:256hz.swapTernary"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "256hz.swapTernary",
				"title": "Swap Ternary"
			}
		],
		"keybindings": [
			{
				"command": "256hz.swapTernary",
				"key": "shift+alt+s"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/sinon": "^9.0.6",
		"@types/vscode": "^1.49.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"sinon": "^9.0.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
  "dependencies": {},
  "repository": {
    "type" : "git",
    "url" : "https://github.com/256hz/SwapTernary"
  }
}
